/ {
    aliases {
        i2c0 = &i2c1;
        uart1 = &usart1;
    };
};

&i2c1 {
    status = "okay";
    clock-frequency = <I2C_BITRATE_STANDARD>;
    bme280@76 {
        compatible = "bosch,bme280";
        reg = <0x76>;
        label = "BME280";
    };
};

&usart1 {
    status = "okay";
    current-speed = <115200>;
};